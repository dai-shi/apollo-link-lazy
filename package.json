{
  "name": "apollo-link-lazy",
  "description": "Apollo Link for lazy loading",
  "version": "0.0.2",
  "author": "Daishi Kato",
  "repository": {
    "type": "git",
    "url": "https://github.com/dai-shi/apollo-link-lazy.git"
  },
  "source": "./src/index.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "browser": "./dist/index.umd.js",
  "types": "./dist/src/index.d.ts",
  "sideEffects": false,
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "compile": "microbundle build --format cjs,es,umd",
    "test": "run-s eslint tsc-test jest",
    "eslint": "eslint --ext .js,.ts,.tsx --ignore-pattern dist .",
    "jest": "jest --preset ts-jest/presets/js-with-ts __tests__/*.ts",
    "tsc-test": "tsc --project . --noEmit",
    "apidoc": "documentation readme --section API --markdown-toc false --parse-extension ts src/*.ts",
    "examples:01_minimal": "DIR=01_minimal EXT=js webpack-dev-server",
    "examples:02_typescript": "DIR=02_typescript webpack-dev-server"
  },
  "keywords": [
    "graphql",
    "apollo",
    "apollo-client",
    "apollo-link",
    "link",
    "lazy"
  ],
  "license": "MIT",
  "dependencies": {
    "apollo-link": "^1.2.13"
  },
  "devDependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@types/jest": "^25.1.4",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "@typescript-eslint/parser": "^2.22.0",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link-schema": "^1.2.4",
    "documentation": "^12.1.4",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "graphql": "^14.6.0",
    "graphql-tag": "^2.10.3",
    "graphql-tools": "^4.0.7",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^25.1.0",
    "microbundle": "^0.12.0-next.7",
    "npm-run-all": "^4.1.5",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "ts-jest": "^25.2.1",
    "ts-loader": "^6.2.1",
    "typescript": "^3.8.3",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  }
}
